{
	"name": "react-stow",
	"version": "1.0.0",
	"description": "Storage abstraction for React",
	"keywords": [
		"react",
		"hooks",
		"storage",
		"local",
		"session",
		"localstorage",
		"sessionstorage"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/dm430/react-stow"
	},
	"author": "Devin Wall",
	"license": "MIT",
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"devDependencies": {
		"@testing-library/react-hooks": "^8.0.1",
		"@types/jest": "^28.1.6",
		"@types/react": "^16.9.56",
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-react": "^7.30.1",
		"jest": "^28.1.3",
		"jest-each": "^28.1.3",
		"jest-environment-jsdom": "^28.1.3",
		"jest-environment-node": "^28.1.3",
		"prettier": "^2.7.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"ts-jest": "^28.0.7",
		"typescript": "^4.7.4"
	},
	"scripts": {
		"build": "tsc",
		"test": "jest",
		"lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"./dist"
	],
	"exports": {
		".": "./dist/index.js",
		"./util": "./dist/util/index.js",
		"./hooks": "./dist/hooks/index.js",
		"./store": "./dist/store/index.js",
		"./event": "./dist/event/index.js",
		"./serialization": "./dist/serialization/index.js",
		"./package.json": "./package.json"
	},
	"typesVersions": {
		"*": {
			".": [
				"./dist/index.d.ts"
			],
			"util": [
				"./dist/util/index.d.ts"
			],
			"hooks": [
				"./dist/hooks/index.d.ts"
			],
			"store": [
				"./dist/store/index.d.ts"
			],
			"event": [
				"./dist/event/index.d.ts"
			],
			"serialization": [
				"./dist/serialization/index.d.ts"
			]
		}
	}
}
